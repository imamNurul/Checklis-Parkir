	<queryString>
		<![CDATA[select tp.*,tk.nama AS pengguna,
(case when aktif is true then 'Active'
else 'Inactive' end) status,
tg.nama as grup
 from tabel_pengguna tp
inner join tabel_karyawan tk on tk.id = tp.id
inner join tabel_grup tg on tg.id = tp.id_grup
where tk.nama like '%$P!{nama}%' AND id_grup like '%$P!{grup}%'
AND tp.aktif IN ($P!{flag})]]>
	</queryString>
